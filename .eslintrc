{
    "extends": [
        "airbnb",
        "prettier",
        "prettier/react"
      ],
    "rules": {
        "react/jsx-filename-extension": [
          1,
          { "extensions": [
            ".js"
            ]
          }
        ],
        "react/jsx-props-no-spreading": 1,
        "react/no-unescaped-entities": 1,
        "import/prefer-default-export": "off"
    },
    "settings": {
      "import/resolver": {
          "node": {
              "moduleDirectory": ["node_modules", "src/"]
          }
      }
    }
}